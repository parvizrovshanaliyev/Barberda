@model  Barbeda.Models.Service
@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPage1.cshtml";
}

<h2>Delete Service </h2>

<form enctype="multipart/form-data" method="post">
    @Html.AntiForgeryToken()
    <div class="form-group">
        @Html.LabelFor(s => s.Name)
        @Html.TextBoxFor(s => s.Name, new { @class = "form-control ", @readonly = "true" })
    </div>
    <div class="form-group">
        @Html.LabelFor(s => s.Description)
        @Html.TextBoxFor(s => s.Description, new { @class = "form-control ", @readonly = "true" })
    </div>
    <div class="form-group">
        @Html.LabelFor(s => s.Price)
        @Html.TextBoxFor(s => s.Price, new { @class = "form-control ", @readonly = "true" })
    </div>
    <div class="form-group">
        @Html.LabelFor(s => s.DataFilter)
        @Html.TextBoxFor(s => s.DataFilter, new { @class = "form-control ", @readonly = "true" })
    </div>

    <input type="submit" class="btn btn-primary" name="name" value="Delete" />
    <a href="~/Admin/Services/Index" class="btn btn-dark">Back to list</a>
</form>

